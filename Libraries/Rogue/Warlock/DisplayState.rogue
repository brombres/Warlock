library Warlock

class DisplayState : State<<DisplayState>>
  METHODS
    method draw
      on_draw
      Display.draw
      on_end_draw

    method on_draw

    method on_end_draw

    method on_end_render

    method on_render

    method on_update

    method on_update( dt:Real )

    method update
      Display.update

    method update( dt:Real )
      on_update( dt )
      Display.update( dt )

endClass
