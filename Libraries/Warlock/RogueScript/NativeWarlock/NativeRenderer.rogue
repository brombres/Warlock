module RogueScript
uses Warlock

class NativeRenderer [singleton]
  METHODS
    method register( vm:VM )
      vm.register_type( "Warlock::Renderer", (v)=>v.object instanceOf Renderer, null,
                        ()=>Renderer, (v)=>Renderer=v.object->(as Renderer)  )
      vm.register_method(
        "Warlock::Renderer.fill(Geometry::Triangle,Graphics::Color)",
        ()=>Renderer.fill(VM.parameter(1)->Triangle,VM.parameter(2)->Color)
      )

endClass
