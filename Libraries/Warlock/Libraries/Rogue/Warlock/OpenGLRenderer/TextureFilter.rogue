augment Warlock::TextureFilter
  METHODS
    method gl_mode->Int32
      which (this)
        case NEAREST:        return native("GL_NEAREST")->Int32
        case LINEAR:         return native("GL_LINEAR")->Int32
        case MIPMAP_NEAREST: return native("GL_NEAREST_MIPMAP_NEAREST")->Int32
        case MIPMAP_LINEAR:  return native("GL_LINEAR_MIPMAP_NEAREST")->Int32
        others:              return native("GL_LINEAR")->Int32
      endWhich
endAugment
